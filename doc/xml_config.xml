<?xml version="1.0" encoding="utf-8"?>
<config>
	<updateserver></updateserver>
	<hostap>
		<ssid mode="1">
			<ssid_name>Basic ssid</ssid_name>
			<mode>b/g/n</mode>
			<channel>0</channel>
			<broadcast_ssid>true/false</broadcast_ssid>
			<radius enable="false"></radius>
			<vlan>
				<dynamic_vlan>disabled</dynamic_vlan>
				<vlan_id>id</vlan_id>
			</vlan>
			<wpa>
				<group_rekey_interval>600</group_rekey_interval>
				<strict_rekey>true/false</strict_rekey>
				<pre_auth>true/false</pre_auth>
				<pre_auth_interfaces>
					<interface>eth0</interface>
					<interface>eth1</interface>
				</pre_auth_interfaces>
				<passphrase>lalalala</passphrase>
			</wpa>
		</ssid>
		<ssid mode="2">
			<ssid_name>Captive portal</ssid_name>
			<mode>b/g/n</mode>
			<channel>0</channel>
			<broadcast_ssid>true/false</broadcast_ssid>
			<radius enable="false"></radius>
			<vlan>
				<dynamic_vlan>disabled</dynamic_vlan>
				<vlan_id>id</vlan_id>
			</vlan>
			<wpa>
				<group_rekey_interval>600</group_rekey_interval>
				<strict_rekey>true/false</strict_rekey>
				<pre_auth>true/false</pre_auth>
				<pre_auth_interfaces>
					<interface>eth0</interface>
					<interface>eth1</interface>
				</pre_auth_interfaces>
				<passphrase>lalalala</passphrase>
			</wpa>
			<portal>
				<mode>url/local</mode>
				<url>http://google.nl</url>
				<local_files>
					<file name="file.html">dataaa</file>
				</local_files>
			</portal>
		</ssid>
		<ssid mode="3">
			<ssid_name>Radius</ssid_name>
			<mode>b/g/n</mode>
			<channel>0</channel>
			<broadcast_ssid>true/false</broadcast_ssid>
			<radius enable="true/false">
				<own_ip>x.x.x.x</own_ip>
				<nas_identifier>??????</nas_identifier>
				<auth_server>
					<ip>x.x.x.x</ip>
					<port>??</port>
					<shared_secret>??????</shared_secret>
				</auth_server>
				<acct_server>
					<ip>x.x.x.x</ip>
					<port>??</port>
					<shared_secret>??????</shared_secret>
					<interim_interval>600</interim_interval>
				</acct_server>
				<retry_interval>600</retry_interval>
			</radius>
			<vlan>
				<dynamic_vlan>disabled/optional/required</dynamic_vlan>
				<vlan_id>id</vlan_id>
			</vlan>
			<wpa>
				<group_rekey_interval>600</group_rekey_interval>
				<strict_rekey>true/false</strict_rekey>
				<pre_auth>true/false</pre_auth>
				<pre_auth_interfaces>
					<interface>eth0</interface>
					<interface>eth1</interface>
				</pre_auth_interfaces>
			</wpa>
			<dhcp_relay>
				<servers>
					<server>x.x.x.x</server>
					<server>x.x.x.x</server>
				</servers>
				<interfaces>
					<interface>eth0</interface>
					<interface>eth1</interface>
				</interfaces>
			</dhcp_relay>
		</ssid>
	</hostap>
	<openvpn>
		<tunnel>
			<server>x.x.x.x</server>
			<port>??</port>
			<cipher>AES</cipher>
			<compression>true/false</compression>
		</tunnel>
	</openvpn>
</config>