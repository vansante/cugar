<?xml version="1.0" encoding="utf-8"?>
<config>
	<hostap>
		<ssid>
			<ssid_name>Accesspoint</ssid_name>
			<mode>b/g/n</mode>
			<channel>0</channel>
			<broadcast_ssid>true/false</broadcast_ssid>
			<radius enable="true/false">
				<own_ip>x.x.x.x</own_ip>
				<nas_identifier>??????</nas_identifier>
				<auth_server>
					<ip>x.x.x.x</ip>
					<port>??</port>
					<shared_secret>??????</shared_secret>
				</auth_server>
				<acct_server>
					<ip>x.x.x.x</ip>
					<port>??</port>
					<shared_secret>??????</shared_secret>
					<interim_interval>600</interim_interval>
				</acct_server>
				<retry_interval>600</retry_interval>
				<dynamic_vlan>disabled/optional/required</dynamic_vlan>
				<vlans>
					<vlan>
						<interface>interface</interface>
						<vlan_id>id</vlan_id>
					</vlan>
					<vlan>
						<interface>interface</interface>
						<vlan_id>id</vlan_id>
					</vlan>
				</vlans>
				<vlan_tagged_interface>eth0</vlan_tagged_interface>
			</radius>
			<wpa>
				<group_rekey_interval>600</group_rekey_interval>
				<strict_rekey>true/false</strict_rekey>
				<pre_auth>true/false</pre_auth>
				<pre_auth_interfaces>
					<interface>eth0</interface>
					<interface>eth1</interface>
				</pre_auth_interfaces>
			</wpa>
		</ssid>
	</hostap>
	<openvpn>
		<tunnel>
			<server>x.x.x.x</server>
			<port>??</port>
			<cipher>AES</cipher>
			<compression>true/false</compression>
		</tunnel>
	</openvpn>
	<dhcp_relay>
		<servers>
			<server>x.x.x.x</server>
			<server>x.x.x.x</server>
		</servers>
		<interfaces>
			<interface>eth0</interface>
			<interface>eth1</interface>
		</interfaces>
	</dhcp_relay>
</config>