<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modes>
		<mode_selection>3</mode_selection>
		<mode3>
            <server>http://www.configurationserver.net</server>
            <public_key>pub.cert</public_key>
            <private_key>priv.key</private_key>
            <certificate>cert.ca.cert</certificate>
        </mode3>
	</modes>
	
	<hardware>
		<mode>g</mode>
		<channel>7</channel>

		<hostname>AP</hostname>
		<address type="static">
			<ip>192.168.1.1</ip>
			<subnet_mask>255.255.255.0</subnet_mask>
			<default_gateway>192.168.1.0</default_gateway>
			<dns_servers>
				<ip>192.168.1.0</ip>
			</dns_servers>
		</address>
	</hardware>
</config>